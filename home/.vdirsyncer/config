[general]
status_path = ~/.vdirsyncer/status/

[pair dav_calendars]
conflict_resolution = b wins
a = calendars_local
b = calendars_dav
collections = ["from a", "from b"]
metadata = ["color", "displayname"]

[pair rs_calendars]
conflict_resolution = b wins
a = calendars_local
b = calendars_rs
collections = ["from a", "from b"]
metadata = ["color", "displayname"]

[storage calendars_local]
type = filesystem
path = ~/.calendars/
fileext = .ics

[storage calendars_dav]
type = caldav
url = https://home.unterwaditzer.net/
username = untitaker
password.fetch = ["command", "vdirsyncer-password", "baikal"]
item_types = []

[storage calendars_rs]
type = remotestorage_calendars
account = untitaker@shack.unterwaditzer.net
access_token.fetch = ["command", "vdirsyncer-password", "remotestorage"]

[pair contacts]
conflict_resolution = b wins
a = contacts_local
b = contacts_remote
collections = null

[storage contacts_local]
type = filesystem
path = ~/.contacts/
fileext = .vcf

[storage contacts_remote]
type = carddav
url = https://home.unterwaditzer.net/baikal/html/dav.php/addressbooks/untitaker/default/
username = untitaker
password.fetch = ["command", "vdirsyncer-password", "baikal"]
